{
    "name":             "@actra-development-oss/redux-observable-decorator",
    "version":          "1.2.0",
    "description":      "Decorators for Redux Observable",
    "main":             "./dist/release/index.js",
    "files":            ["dist/release"],
    "typings":          "./dist/release/index.d.ts",
    "scripts":          {
        "test":             "karma start --single-run",
        "build":            "npm run build:js",
        "build:js":         "npm run ngc -- -p ./tsconfig.dist.json",
        "prebuild":         "npm run test && npm run clean:pre",
        "postbuild":        "npm run clean:post",
        "clean:pre":        "npm run clean:dist",
        "clean:post":       "npm run rimraf -- ./**/*.ngsummary.json",
        "postversion":      "npm run build",
        "clean:dist":       "npm run rimraf -- dist",
        "preparepublish":   "npm run preparepublish:1 && npm run preparepublish:2 && npm run preparepublish:3 && npm run preparepublish:4",
        "preparepublish:1": "npm run ncp -- ./CHANGELOG.MD ./dist/CHANGELOG.MD",
        "preparepublish:2": "npm run ncp -- ./LICENSE ./dist/LICENSE",
        "preparepublish:3": "npm run ncp -- ./package.dist.json ./dist/package.json",
        "preparepublish:4": "npm run ncp -- ./README.md ./dist/README.md",
        "publishToNpmjs":   "npm run build && npm run preparepublish && npm publish ./dist --access public --registry https://registry.npmjs.com/ && npm run clean:dist",
        "mkdirp":           "./node_modules/.bin/mkdirp",
        "ncp":              "./node_modules/.bin/ncp",
        "ngc":              "./node_modules/.bin/ngc",
        "rimraf":           "./node_modules/.bin/rimraf"
    },
    "repository":       "git+https://github.com/actra-development-oss/redux-observable-decorator.git",
    "author":           "Evan Schultz",
    "contributors":     [
        {
            "name":  "Gabriel Schuster - actra.development",
            "email": "opensource@actra.de",
            "url":   "http://www.actra.de/velopment"
        }
    ],
    "license":          "MIT",
    "bugs":             {
        "url": "https://github.com/actra-development-oss/redux-observable-decorator/issues"
    },
    "homepage":      "https://github.com/actra-development-oss/redux-observable-decorator#readme",
    "publishConfig":    {
        "registry": "https://registry.npmjs.com/",
        "access":   "public"
    },
    "peerDependencies": {
        "redux":            "3.*",
        "redux-observable": "^0.16.0"
    },
    "devDependencies":  {
        "@angular/animations":               "^4.3.6",
        "@angular/common":                   "^4.3.6",
        "@angular/compiler":                 "^4.3.6",
        "@angular/compiler-cli":             "^4.3.6",
        "@angular/core":                     "^4.3.6",
        "@angular/platform-browser":         "^4.3.6",
        "@angular/platform-browser-dynamic": "^4.3.6",
        "@angular/platform-server":          "^4.3.6",
        "@types/jasmine":                    "^2.5.53",
        "@types/node":                       "^8.0.25",
        "awesome-typescript-loader":         "^3.2.3",
        "core-js":                           "^2.5.0",
        "istanbul-instrumenter-loader":      "^3.0.0",
        "jasmine-core":                      "^2.7.0",
        "karma":                             "^1.7.0",
        "karma-chrome-launcher":             "^2.2.0",
        "karma-coverage":                    "^1.1.1",
        "karma-jasmine":                     "^1.1.0",
        "karma-mocha-reporter":              "^2.2.3",
        "karma-sourcemap-loader":            "^0.3.7",
        "karma-typescript-preprocessor":     "^0.3.1",
        "karma-webpack":                     "^2.0.4",
        "mkdirp":                            "^0.5.1",
        "ncp":                               "^2.0.0",
        "redux":                             "^3.7.2",
        "redux-observable":                  "^0.16.0",
        "reflect-metadata":                  "^0.1.10",
        "rimraf":                            "^2.6.1",
        "rxjs":                              "^5.4.3",
        "ts-loader":                         "^2.3.4",
        "tslint":                            "^5.7.0",
        "tslint-loader":                     "^3.5.3",
        "typescript":                        "^2.5.1",
        "uglify-js":                         "^3.0.28",
        "webpack":                           "^3.4.1",
        "webpack-dev-middleware":            "^1.12.0",
        "zone.js":                           "^0.8.17"
    }
}
